{
  "name": "gym-ai-coach-v2",
  "private": true,
  "version": "2.1.0",
  "description": "AI-powered fitness coach with advanced computer vision and personalized plan generation",
  "type": "module",
  "keywords": ["fitness", "ai", "computer-vision", "nutrition", "workout-plans", "pose-detection"],
  "author": "Gym AI Coach Team",
  "license": "MIT",
  "homepage": "https://gymaicoach.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/gym-ai-coach.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/gym-ai-coach/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npx vite-bundle-analyzer dist/stats.html",
    "docker:build": "docker build -f Dockerfile-frontend -t gym-ai-coach-frontend .",
    "docker:run": "docker run -p 3000:3000 gym-ai-coach-frontend"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3",
