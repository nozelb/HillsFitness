    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.18.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
